//import solidityf

pragma solidity ^0.4.0;

contract simpleContract {
  // Arithmetic operators + - * / % **
  uint a = 2;
  uint b = 3;

  uint c = a ** b // power = 8

  //logical operators ! NOT, && AND, || OR, ==, !=
  bool hasMoney = !false;

  //bitwise operators --> logical exclusively for bytes AND &, | OR, ^ EXCLUSIVE OR, ~ NEGATION, << bitwise left shift, >> bitwise left shift
  bytes1 contractValue = 0x02 | 0x01;
  



}   



